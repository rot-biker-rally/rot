{
    "name": "rot-biker-rally",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rot-biker-rally/rot-rally.git"
    },
    "author": "Vicious Chicken",
    "license": "GPL-3.0-or-later",
    "devDependencies": {
        "autoprefixer": "^9.2.1",
        "node-sass": "^4.9.4",
        "postcss-cli": "^6.0.1"
    },
    "scripts": {
        "sass": "node-sass --output-style expanded themes/enfold-child-2/scss/style.scss themes/enfold-child-2/dist/css/style.css",
        "postsass": "postcss themes/enfold-child-2/dist/css/ --use autoprefixer --no-map --dir themes/enfold-child-2/dist/css/",
        "watch:sass": "watch 'npm run sass' themes/enfold-child-2/scss/"
    }
}
